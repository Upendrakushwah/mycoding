#include <iostream>
#include <string>

bool isSubsequence(const std::string& s, const std::string& t) {
    size_t i = 0, j = 0;
    while (i < s.length() && j < t.length()) {
        if (s[i] == t[j]) {
            ++i;
        }
        ++j;
    }
    return i == s.length();
}

int main() {
    std::string s = "ace";
    std::string t = "abcde";
    std::cout << (isSubsequence(s, t) ? "True" : "False") << std::endl;  // Output: True

    s = "aec";
    t = "abcde";
    std::cout << (isSubsequence(s, t) ? "True" : "False") << std::endl;  // Output: False

    return 0;
}
